
 <p><%= link_to "プロフィール編集", edit_user_path(current_user)%></p>
<% @users.each do |user| %>
<div>
  <p>name<%= user.name %></p>
  <p>image</p>
  <% if user.image.attached? %>
    <%= image_tag user.image, size: "200x200" %>
  <% else %>
    <%= image_tag 'no_image', size: "200x200" %>
  <% end %>
  
</div>
<% end %>
